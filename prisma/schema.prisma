generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  books Book[]
}

model Book {
  id Int @id @default(autoincrement())
  title String
  author String
  genre String
  status String
  userId Int
  user User @relation(fields: [userId], references: [id])
}
